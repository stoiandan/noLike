{
    "manifest_version": 2,
    "name": "noLike",
    "version": "1.0",
  
    "description": "Disable Facebook media buttons (like, react, comment, share, etc.)",
  
    "icons": {
      "48": "icons/noLike.png"
    },
  
    "content_scripts": [
      {
        "matches": ["*://*.facebook.com/*"],
        "js": ["src/nolike.js"]
      }
    ]
  
  }